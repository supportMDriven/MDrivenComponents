        <Method Name="CreateCombinedOutgoingNotification" CodeComment="" IsAbstract="False" IsConstructor="False" IsNew="False" IsOverride="False" IsQuery="False" IsSealed="False" IsStatic="False" IsVirtual="False" Postcondition="" Precondition="self.ActiveNotificationsForMe-&gt;select(un|un.Notification.CanBeCombined and un.LastOutgoingMessage.isNull)-&gt;notEmpty" Visibility="Public" id="a05405f2-743b-4724-879f-e51c18b801fd">
